<div class="col-xs-4 product-container">
	<a href="#" class="product thumbnail">
		<img src="{{product.img}}" alt="{{product.name}}">
	</a>
	<div class="caption">
		<h4>{{product.name}} (S/N:{{product.sku}})</h4>
		<p>{{product.desc}}</p>
	</div>
	<div>
		<p class="text-center"><a href="#" class="btn add-cart-btn" role="button">Add to Cart</a></p>
	</div>
</div>
